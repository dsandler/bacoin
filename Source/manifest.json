{
	"manifest_version": 2,
	"name": "Bacoin",
	"version": "1.0",
	"description": "Replaces the text 'bitcoin' with 'bacon', and vice versa.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": { "128": "icon128.png",
	            "48": "icon48.png" }
}

